{
  "name": "chatgpt",
  "productName": "ChatGPT",
  "displayName": "ChatGPT",
  "type": "module",
  "author": "OpenAI",
  "version": "1.2025.328",
  "private": true,
  "description": "ChatGPT Desktop Application",
  "main": ".vite/build/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:openai/windows-desktop"
  },
  "scripts": {
    "start": "npm run i18n-extract && electron-forge start",
    "start:packaged": "cross-env LAUNCH_MAKE_ARTIFACT=true DEBUG=\"electron-forge:*\" electron-forge make",
    "dev": "cross-env SIDETRON_DEV_HOST=\"http://localhost:3000\" CES_HOST=\"http://localhost:7009\" npm run start",
    "staging": "cross-env SIDETRON_DEV_HOST=\"https://chatgpt-staging.com\" CES_HOST=\"https://chatgpt-staging.com:7009\" npm run start",
    "dev:remote": "concurrently -n socat,npm -i --default-input-target npm \"node --loader ts-node/esm scripts/remote/windows-remote-dev.ts\" \"npm run dev\"",
    "dev:host": "concurrently -n socat,tilt \"node --loader ts-node/esm scripts/remote/setup-macos-for-remote.ts\" \"az account clear; az login; cd ~/code/openai; applied tilt up;\"",
    "package": "cross-env DEBUG=\"electron-forge:*\" electron-forge package",
    "make": "cross-env DEBUG=\"electron-forge:*\" electron-forge make",
    "make:launch": "cross-env LAUNCH_MAKE_ARTIFACT=true DEBUG=\"electron-forge:*\" electron-forge make",
    "make:signed": "cross-env ENABLE_CODESIGNING=true DEBUG=\"electron-forge:*\" electron-forge make",
    "publish": "cross-env DEBUG=\"electron-forge:*\" electron-forge publish",
    "lint:markdown": "markdownlint-cli2 \"*.md\"",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx,.cts,.cjs,mjs,mts .",
    "lint:fix": "npm-run-all \"lint:!(fix) -- --fix\"",
    "lint": "npm-run-all \"lint:!(fix)\"",
    "check:lint": "npm run lint",
    "check:types": "tsc --noEmit --project .",
    "check:electronegativity": "node scripts/exec-electronegativity.js",
    "check": "npm-run-all --parallel \"check:!(fix)\"",
    "i18n-extract": "cross-env formatjs extract \"./src/**/*.{ts,tsx}\" --out-file \"./src/common/locales-mojito/en.json\"",
    "i18n-compile-mojito": "cross-env formatjs compile-folder \"src/common/locales-mojito/\" \"src/common/locales/\" && node -e \"require('fs').unlinkSync('src/common/locales/en.json');\"",
    "manual-symbolicate": "node --loader ts-node/esm scripts/manual-symbolicate.ts",
    "sync": "node --loader ts-node/esm scripts/repo-sync.ts",
    "test": "vitest --typecheck",
    "test:coverage": "vitest run --coverage --typecheck",
    "symbol-upload": "sentry-cli debug-files upload -o openai -p 4507771614265344 packages"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.27.1",
    "@datadog/datadog-api-client": "^1.41.0",
    "@doyensec/electronegativity": "^1.10.3",
    "@electron-forge/cli": "7.9.0",
    "@electron-forge/maker-zip": "7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.9.0",
    "@electron-forge/plugin-electronegativity": "7.9.0",
    "@electron-forge/plugin-fuses": "7.9.0",
    "@electron-forge/plugin-vite": "7.9.0",
    "@electron-forge/shared-types": "7.9.0",
    "@electron/fuses": "^1.8.0",
    "@electron/lint-roller": "^3.1.2",
    "@formatjs/cli": "^6.7.2",
    "@segment/analytics-node": "^2.3.0",
    "@sentry/cli": "^2.53.0",
    "@sentry/electron": "^6.8.0",
    "@sentry/vite-plugin": "^4.2.0",
    "@statsig/js-client": "^3.25.3",
    "@statsig/js-local-overrides": "^3.25.3",
    "@statsig/react-bindings": "^3.25.3",
    "@types/cross-zip": "^4.0.2",
    "@types/expand-tilde": "^2.0.2",
    "@types/glob": "^8.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "async-mutex": "^0.5.0",
    "babel-plugin-formatjs": "^10.5.39",
    "commander": "4.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "cross-zip": "^4.0.1",
    "electron": "37.4.0",
    "electron-log": "^5.4.3",
    "electron-windows-msix": "^1.1.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-formatjs": "^4.13.3",
    "eslint-plugin-import-x": "^4.9.4",
    "eslint-plugin-path": "^1.4.0-beta.0",
    "eslint-plugin-prettier": "^5.2.5",
    "exec-powershell": "^1.0.1",
    "expand-tilde": "^2.0.2",
    "markdownlint-cli2": "^0.18.1",
    "npm-run-all": "^4.1.5",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.6.2",
    "react-intl": "^7.1.11",
    "sharp": "^0.34.3",
    "source-map": "^0.7.6",
    "statsig-js": "^5.1.0",
    "systeminformation": "^5.27.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.0.5",
    "vite": "^6.3.6",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-string": "^1.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2",
    "zustand": "^5.0.8"
  },
  "keywords": [],
  "packageManager": "npm@10.8.1",
  "windowsKitVersion": "10.0.22621.0",
  "workspaces": [
    "packages/*"
  ]
}
